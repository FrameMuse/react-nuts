{
  "compilerOptions": {
    "composite": true,
    "experimentalDecorators": true,
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "target": "ES5",
    "baseUrl": "src",
    "lib": [
      "dom",
      "ES5",
      "ES6",
      "ESNext"
    ],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noStrictGenericChecks": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "removeComments": true,
    "sourceMap": true,
    "noEmit": true,
    "emitDecoratorMetadata": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "checkJs": false
  },
  "typeAcquisition": {
    "include": [
      "node"
    ]
  },
  "include": [
    "src/**/*",
    "src/**/*.json",
    "node_modules/@framemuse"
  ]
}